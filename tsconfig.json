{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@styles/*": ["src/styles/*"],
      "@press/*": ["src/pages/[press]/*"],
      "@projects/*": ["src/pages/[projects]/*"],
      "@root/*": ["src/*"],
    },
  },
}